{
  "name": "beanstalkify",
  "version": "3.0.0",
  "description": "AWS Elastic Beanstalk automation",
  "repository": {
    "type": "git",
    "url": "https://github.com/liamqma/beanstalkify.git"
  },
  "engines": {
    "node": ">=18.18.2"
  },
  "main": "dist/index.js",
  "files": [
    "README.md",
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose --runInBand --unhandled-rejections=strict --coverage",
    "coveralls": "lcov-result-merger 'coverage/**/lcov.info' | ./node_modules/.bin/coveralls",
    "formatting:check": "prettier --check ./src",
    "formatting:write": "prettier --write ./src",
    "lint": "eslint ./src --ext .ts,.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-elastic-beanstalk": "^3.428.0",
    "@aws-sdk/client-s3": "^3.428.0",
    "@faker-js/faker": "^8.2.0",
    "axios": "^1.5.1",
    "lodash": "^4.17.21",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
